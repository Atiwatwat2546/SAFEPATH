{
  "expo": {
    "name": "safepath-expo",
    "slug": "safepath-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "safepathexpo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store"
    ]
  }
}
